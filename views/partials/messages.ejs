<% if(typeof errors != 'undefined' && errors.length >0){ %>
    <% errors.forEach(function(error){ %>
        <div class="alert alert-warning " role="alert">
            <%= error.msg %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"> X </button>
          </div>
    <% }); %>
<% } %> 

